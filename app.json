{
  "expo": {
    "name": "RoutineQuest",
    "slug": "RoutineQuest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/portada_rq.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/portada_rq.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.josedabh.RoutineQuest",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomain": false
            }
          }
        }
      },
      "usesAppleSignIn": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/portada_rq.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.josedabh.RoutineQuest",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "FOREGROUND_SERVICE",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK"
      ],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "usesCleartextTraffic": true,
      "softwareKeyboardLayoutMode": "resize"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#FFFFFF"
    },
    "web": {
      "favicon": "./assets/portada_rq.png"
    },
    "extra": {
      "eas": {
        "projectId": "500ce1c7-bc44-48a7-acdb-79fe144f264b"
      },
      "API_URL": "http://200.58.107.89:8080"
    },
    "plugins": [
      "expo-build-properties"
    ]
  }
}